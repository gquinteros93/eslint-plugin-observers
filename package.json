{
  "name": "eslint-plugin-observers",
  "version": "1.0.0",
  "description": "ESLint plugin for preventing memory leaks around observers (ResizeObserver, IntersectionObserver, MutationObserver) test",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "npm run build && node tests/lib/rules/no-missing-unobserve-or-disconnect.js && node tests/lib/rules/matching-unobserve-target.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gquinteros93/eslint-plugin-observers.git"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "event",
    "observers",
    "ResizeObserver",
    "IntersectionObserver",
    "MutationObserver"
  ],
  "author": "German Quinteros",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gquinteros93/eslint-plugin-observers/issues"
  },
  "homepage": "https://github.com/gquinteros93/eslint-plugin-observers",
  "private": true,
  "peerDependencies": {
    "eslint": ">=0.8.0"
  },
  "dependencies": {
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@types/eslint": "^6.8.1",
    "@types/estree": "0.0.39",
    "@types/lodash": "4.14.179",
    "@types/node": "^14.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-eslint-plugin": "^5.0.8",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "precommit": [
    "lint"
  ]
}
